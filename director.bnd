Bundle-ClassPath: testng.jar,clover.jar,.

Include-Resource:\
 testng.jar=${testng.jar},\
 clover.jar=${clover.jar}

Private-Package:\
 org.ops4j.peaberry.test

Export-Package: *

Import-Package:\
 org.osgi.framework; version="[1.3.0, 2.0.0)",\
 org.osgi.util.tracker; version="[1.3.0, 2.0.0)",\
 *; resolution:=optional

Bundle-Activator:\
 org.ops4j.peaberry.test.TestActivator

-output:\
 ${director.jar}

