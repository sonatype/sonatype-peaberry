<?xml version="1.0"?>
<project name="peaberry-eclipse">

  <property name="antlib" value="../../lib/ant"/>
  <import file="../../setup/common.xml"/>
  <property file="build.properties"/>

  <target name="jar" depends="common.jar">
    <makeurl property="deploy.urls">
      <fileset dir="${lib}" includes="*.jar"/>
      <fileset dir="../../build/dist" includes="*.jar" excludes="*-sources-*,*-javadocs-*"/>
      <fileset dir="${dist}" includes="*.jar" excludes="*-sources-*,*-javadocs-*"/>
    </makeurl>
  </target>

</project>
